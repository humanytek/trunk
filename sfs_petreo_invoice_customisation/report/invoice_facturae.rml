      <blockTable colWidths="50.0,39.0,175.0,101.0,50.0,83.0" style="Table9">
        <tr>
          <td>
            <para style="P9">CANTIDAD</para>
          </td>
          <td>
            <para style="P9">UNIDAD</para>
          </td>
          <td>
            <para style="P9">DESCRIPCIÓN</para>
          </td>
          <td>
            <para style="P9">P.UNITARIO</para>
          </td>
          <td>
            <para style="P9">DTO %</para>
          </td>
          <td>
            <para style="P9">IMPORTE</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P12">[[ repeatIn(o.invoice_line,'l') ]] [[ formatLang(l.quantity) ]]</para>
          </td>
          <td>
            <para style="P12">[[ (l.uos_id and l.uos_id.name) or '' ]]</para>
          </td>
          <td>
            <para style="P12">[[ l.name ]]</para>
            <para style="P12">Notas: [[l.note or removeParentNode('para')]]</para>
          </td>
          <td>
            <para style="P12">[[ formatLang(l.price_unit) ]]</para>
          </td>
          <td>
            <para style="P12">[[ formatLang(l.discount)]]%</para>
          </td>
          <td>
            <para style="P12">[[ exists_key('global_discount_percent') and (formatLang(l.quantity * l.price_unit, digits=get_digits(dp='Account'))) or formatLang(l.price_subtotal) ]]</para>
          </td>
        </tr>
      </blockTable>
      <blockTable colWidths="416.0,82.0" style="Table10">
        <tr>
          <td>
            <para style="P10">MONEDA</para>
          </td>
          <td>
            <para style="P13">[[ o.currency_id.name ]]</para>
          </td>
        </tr>
        <tr>
          <td>

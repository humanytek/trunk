  <story>
    <section>
      <para style="P31">[[ repeatIn(objects,'o') ]] <font face="Helvetica" size="9.0">[[ set_global_data(o) ]]</font></para>
      <blockTable colWidths="141.0,193.0,90.0,129.0" style="Table3">
        <tr>
          <td>
            <para style="P32">[[ o.company_id.logo and setTag('para','image',{'width':'130.0','height':'80.0'}) ]] [[ o.company_id.logo ]]</para>
          </td>
          <td>
            <para style="P37">[[ o.company_id.parent_id and o.company_id.parent_id.partner_id.name or o.company_id.partner_id.name ]]</para>
            <para style="P36">Calle: [[ company_address()['street'] or '' ]] </para>
            <para style="P36">Ciudad: [[ company_address()['city'] or '' ]], Estado: [[ company_address()['state_id'] and company_address()['state_id']['name'] or '' ]] </para>
            <para style="P36">Localidad: [[ company_address()['city2'] or '' ]]</para>
            <para style="P36">CP: [[ company_address()['zip'] or '' ]]</para>
            <para style="P33">
              <font face="Helvetica">Teléfono(s): </font>
              <font face="Helvetica">[[ company_address()['phone'] or removeParentNode('para') ]]</font>
            </para>
          </td>
          <td>
            <para style="P1">[[ o.company_id.cif_file and setTag('para','image',{'width':'98.0','height':'161.0'}) ]] [[ o.company_id.cif_file ]]</para>
          </td>
          <td>
            <para style="P3">Folio:[[ get_approval() and get_approval().serie or '' ]][[ ( o.type in ['out_invoice', 'out_refund'] ) and ( o.state in ['open', 'paid', 'cancel'] ) and o.number or 'SIN FOLIO O ESTATUS NO VALIDO' ]]<font face="Helvetica"/></para>
            <para style="P4">
              <font color="white"> </font>
            </para>
            <para style="P4">
              <font color="white"> </font>
            </para>
            <para style="P40">FOLIO SAT</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P24">[[ split_strings( o.cfdi_folio_fiscal or '') ]]</para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P24">
              <font color="white"> </font>
            </para>
            <para style="P9">APROBACIÓN</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P5">[[ get_approval() and get_approval().approval_number or '' ]]</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P9">AÑO APROBACIÓN</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P5">[[ get_approval() and get_approval().approval_year or '' ]]</para>
          </td>
        </tr>
      </blockTable>
      <para style="P42">[[ repeatIn(objects,'o') ]] [[ set_global_data(o) ]]</para>
      <blockTable colWidths="95.0,223.0,181.0" style="Table1">
        <tr>
          <td>
            <para style="P8">EMISOR</para>
          </td>
          <td>
            <para style="P11">[[ o.partner_id.name ]]</para>
          </td>
          <td>
            <para style="P9">DIRECCIÓN DE EXPEDICIÓN</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P8">RFC EMISOR</para>
          </td>
          <td>
            <para style="P34">[[ o.company_id.partner_id._columns.has_key('vat_split') and o.company_id.partner_id.vat_split or o.company_id.partner_id.vat ]]</para>
          </td>
          <td>
            <para style="P35">[[ company_address()['street'] or '' ]] ,[[ company_address()['city'] or '' ]],[[ company_address()['state_id'] and company_address()['state_id']['name'] or '' ]] ,[[ company_address()['zip'] or '' ]]</para>
          </td>
        </tr>
        <tr>
          <td>

          </td>
          <td>
            <para style="P13">[[ o.rate ]]</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P10">SUBTOTAL</para>
          </td>
          <td>
            <para style="P7">
              <font face="Helvetica" size="8.0">$</font>
              <font face="Helvetica" size="8.0">[[ formatLang(o.amount_untaxed) ]]</font>
            </para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P28">[[ repeatIn( get_taxes(), 'tax' ) ]] [[ tax['name2'] ]] ([[ round( float( tax['tax_percent'] ) ) or '0.0' ]]%) </para>
          </td>
          <td>
            <para style="P7">
              <font face="Helvetica" size="8.0">$</font>
              <font face="Helvetica" size="8.0">[[ formatLang( float( tax['amount'] ) ) ]]</font>
            </para>
          </td>
        </tr>

    <section>
      <para style="P32">[[ repeatIn(objects,'o') ]] <font face="Helvetica" size="9.0">[[ set_global_data(o) ]]</font></para>
      <blockTable colWidths="146.0,193.0,96.0,123.0" style="Table3">
        <tr>
          <td>
            <para style="P33">[[ o.company_id.logo and setTag('para','image',{'width':'140.0','height':'40.0'}) ]] [[ o.company_id.logo ]]</para>
          </td>
          <td>
            <para style="P38">[[ o.company_id.parent_id and o.company_id.parent_id.partner_id.name or o.company_id.partner_id.name ]]</para>
            <para style="P37">Calle: [[ company_address()['street'] or '' ]] </para>
            <para style="P37">Ciudad: [[ company_address()['city'] or '' ]], Estado: [[ company_address()['state_id'] and company_address()['state_id']['name'] or '' ]] </para>
            <para style="P37">CP: [[ company_address()['zip'] or '' ]]</para>
            <para style="P34">
              <font face="Helvetica">Teléfono(s): </font>
              <font face="Helvetica">[[ company_address()['phone'] or removeParentNode('para') ]]</font>
            </para>
          </td>
          <td>
            <para style="P1">[[ o.company_id.cif_file and setTag('para','image',{'width':'98.0','height':'161.0'}) ]] [[ o.company_id.cif_file ]]</para>
          </td>
          <td>
            <para style="P3">Folio:[[ get_approval() and get_approval().serie or '' ]][[ ( o.type in ['out_invoice', 'out_refund'] ) and ( o.state in ['open', 'paid', 'cancel'] ) and o.number or 'SIN FOLIO O ESTATUS NO VALIDO' ]]<font face="Helvetica"/></para>
            <para style="P4">
              <font color="white"> </font>
            </para>
            <para style="P4">
              <font color="white"> </font>
            </para>
            <para style="P16">FOLIO SAT</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P27">[[ split_strings( o.cfdi_folio_fiscal or '') ]]</para>
            <para style="P27">
              <font color="white"> </font>
            </para>
            <para style="P27">
              <font color="white"> </font>
            </para>
            <para style="P9">APROBACIÓN</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P5">[[ get_approval() and get_approval().approval_number or '' ]]</para>
            <para style="P5">
              <font color="white"> </font>
            </para>
            <para style="P9">AÑO APROBACIÓN</para>
            <para style="P5">
              <font color="white"> </font>
